{
  "name": "silver-invention-ui",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "extract-i18n": "cd src && ng-xi18n",
    "build-i18n": "for lang in en es fr; do ng build -o dist/$lang --aot -prod --bh /silver-invention-ui/$lang/ --i18n-file=src/app/i18n/messages.$lang.xlf --i18n-format=xlf --locale=$lang; done",
    "build-gh-pages": "for lang in en es fr; do ng build -o docs/$lang --aot -prod --bh /silver-invention-ui/$lang/ --i18n-file=src/app/i18n/messages.$lang.xlf --i18n-format=xlf --locale=$lang; done"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/material": "^2.0.0-beta.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.3.1",
    "angular-in-memory-web-api": "^0.2.4",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.0.3",
    "ts-helpers": "^1.1.2",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.26",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.4.0",
    "protractor": "~4.0.13",
    "ts-node": "2.0.0",
    "tslint": "^4.0.2",
    "typescript": "~2.1.5"
  }
}
